          "value": "index,follow"
        },
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/api/demo/access",
      "destination": "/api/demo/access"
    },
    {
      "source": "/api/demo/exchange",
      "destination": "/api/demo/exchange"
    },
    {
      "source": "/api/demo/static",
      "destination": "/api/demo/static"
    },
    {
      "source": "/demo",
      "destination": "/api/demo/index"
    },
    {
      "source": "/demo/tasks",
      "destination": "/api/demo/tasks"
    },
    {
      "source": "/demo/habits",
      "destination": "/api/demo/habits"
    },
    {
      "source": "/demo/journal",
      "destination": "/api/demo/journal"
    },
    {
      "source": "/demo/review",
      "destination": "/api/demo/review"
    },
    {
      "source": "/demo/agenda",
      "destination": "/api/demo/agenda"
    },
    {
      "source": "/demo/settings",
      "destination": "/api/demo/settings"
    }
  ]
}